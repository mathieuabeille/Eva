<!-- <p id="notice"><%= notice %></p> -->
<center>
  <h1>Tableau de bord</h1>

  <table>
    <thead>
      <tr>
        <th>Prénom</th>
        <th>Nom</th>
        <th>Email</th>
        <th>Entreprise</th>
        <th>Poste</th>
        <th>Téléphone</th>
        <th>Message</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @annonceurs.each do |annonceur| %>
        <tr>
          <td><% if annonceur.present? %><%= annonceur.first_name %> <% else %> <% end %></td>
          <td><% if annonceur.present? %><%= annonceur.last_name %> <% else %> <% end %></td>
          <td><% if annonceur.present? %><%= annonceur.mail %> <% else %> <% end %></td>
          <td><% if annonceur.present? %><%= annonceur.company %> <% else %> <% end %></td>
          <td><% if annonceur.present? %><%= annonceur.job_title %> <% else %> <% end %></td>
          <td><% if annonceur.present? %><%= annonceur.phone %> <% else %> <% end %></td>
          <td><%= link_to 'Edit', edit_annonceur_path(annonceur) %></td>
          <td><%= link_to 'Destroy', annonceur, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          <td><% if annonceur.present? %><%= annonceur.message %> <% else %> <% end %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</center>

